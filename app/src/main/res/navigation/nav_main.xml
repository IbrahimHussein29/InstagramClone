<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"

    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.sec.instagramclone.ui.main.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.sec.instagramclone.ui.main.search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />
    <dialog
        android:id="@+id/uploadFragment"
        android:name="com.sec.instagramclone.ui.main.upload.UploadFragment"
        android:label="fragment_add"
        tools:layout="@layout/fragment_upload" />
    <fragment
        android:id="@+id/reelsFragment"
        android:name="com.sec.instagramclone.ui.main.reels.ReelsFragment"
        android:label="fragment_reels"
        tools:layout="@layout/fragment_reels" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.sec.instagramclone.ui.main.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
    </fragment>
    <fragment
        android:id="@+id/updateProfileFragment"
        android:name="com.sec.instagramclone.ui.main.profile.UpdateProfileFragment"
        android:label="fragment_update_profile"
        tools:layout="@layout/fragment_update_profile" />
    <fragment
        android:id="@+id/profilePostsFragment"
        android:name="com.sec.instagramclone.ui.main.profile.profilePosts.ProfilePostsFragment"
        android:label="fragment_profile_posts"
        tools:layout="@layout/fragment_profile_posts" />
    <fragment
        android:id="@+id/profileReelsFragment"
        android:name="com.sec.instagramclone.ui.main.profile.profileReels.ProfileReelsFragment"
        android:label="fragment_profile_reels"
        tools:layout="@layout/fragment_profile_reels" />
    <fragment
        android:id="@+id/uploadPostFragment"
        android:name="com.sec.instagramclone.ui.main.upload.UploadPostFragment"
        android:label="fragment_add_post"
        tools:layout="@layout/fragment_upload_post" />
    <fragment
        android:id="@+id/uploadReelsFragment"
        android:name="com.sec.instagramclone.ui.main.upload.UploadReelsFragment"
        android:label="fragment_add_reels"
        tools:layout="@layout/fragment_upload_reels" />


</navigation>